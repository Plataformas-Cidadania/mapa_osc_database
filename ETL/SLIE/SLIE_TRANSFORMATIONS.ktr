jeto_externo, ft_identificador_projeto_externo, bo_oficial&#x29;&#xd;&#xa;select id_osc, &#x27;&#x3f;&#x27;,&#x27;&#x3f;&#x27;,&#x3f;,&#x27;&#x3f;&#x27;,CASE WHEN length&#x28;&#x27;&#x3f;&#x27;&#x29; &#x3e; 0 THEN to_date&#x28;&#x27;&#x3f;&#x27;,&#x27;DD&#x2f;MM&#x2f;YYYY&#x27;&#x29; ELSE NULL END as dt_ini,&#x27;&#x3f;&#x27;,CASE WHEN length&#x28;&#x27;&#x3f;&#x27;&#x29; &#x3e; 0 THEN to_date&#x28;&#x27;&#x3f;&#x27;,&#x27;DD&#x2f;MM&#x2f;YYYY&#x27;&#x29; ELSE NULL END as dt_fim,&#x27;&#x3f;&#x27;,&#x3f; &#x2b; &#x3f;,&#x27;&#x3f;&#x27;,replace&#x28;&#x27;&#x3f;&#x27;,&#x27;,&#x27;,&#x27;.&#x27;&#x29;&#x3a;&#x3a;DOUBLE PRECISION,&#x27;&#x3f;&#x27;, replace&#x28;&#x27;&#x3f;&#x27;,&#x27;,&#x27;,&#x27;.&#x27;&#x29;&#x3a;&#x3a;DOUBLE PRECISION, &#x27;&#x3f;&#x27;, &#x27;&#x3f;&#x27;, &#x27;&#x3f;&#x27;, &#x27;&#x3f;&#x27;, &#x27;&#x3f;&#x27;, true&#xd;&#xa;from osc.tb_osc&#xd;&#xa;where cd_identificador_osc &#x3d; &#x3f;&#xd;&#xa;AND NOT EXISTS &#x28;SELECT &#x2a; FROM osc.tb_projeto WHERE osc.tb_projeto.tx_identificador_projeto_externo &#x3d; &#x27;&#x3f;&#x27; AND ft_identificador_projeto_externo &#x3d; &#x27;&#x3f;&#x27;&#x29;&#xd;&#xa;RETURNING id_projeto&#x29;&#xd;&#xa;&#xd;&#xa;INSERT INTO osc.tb_localizacao_projeto&#x28;id_projeto,tx_nome_regiao_localizacao_projeto,id_regiao_localizacao_projeto,ft_regiao_localizacao_projeto,ft_nome_regiao_localizacao_projeto, bo_oficial&#x29;&#xd;&#xa;SELECT id_projeto, &#x27;&#x3f;&#x27;,edmu_cd_municipio, &#x27;&#x3f;&#x27; ,&#x27;&#x3f;&#x27;, true&#xd;&#xa;FROM rows&#xd;&#xa;INNER JOIN spat.ed_municipio on UNACCENT&#x28;UPPER&#x28;edmu_nm_municipio&#x29;&#x29; &#x3d; UNACCENT&#x28;UPPER&#x28;&#x27;&#x3f;&#x27;&#x29;&#x29; AND eduf_cd_uf &#x3d; &#x28;SELECT eduf_cd_uf FROM spat.ed_uf WHERE UPPER&#x28;eduf_sg_uf&#x29; &#x3d; UPPER&#x28;&#x27;&#x3f;&#x27;&#x29;&#x29;&#x3b;&#xd;&#xa;</sql>
