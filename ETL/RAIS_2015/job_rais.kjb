      <description/>
      <type>SQL</type>
      <sql>INSERT INTO syst.dc_natureza_juridica&#x28;&#xd;&#xa;            cd_natureza_juridica, tx_nome_natureza_juridica&#x29;&#xd;&#xa;SELECT 3271, &#x27;&#xd3;rg&#xe3;o de Dire&#xe7;&#xe3;o Local de Partido Pol&#xed;tico&#x27;&#xd;&#xa;FROM syst.dc_natureza_juridica&#xd;&#xa;WHERE NOT EXISTS &#x28;SELECT &#x2a; FROM syst.dc_natureza_juridica WHERE cd_natureza_juridica &#x3d; 3271&#x29;&#xd;&#xa;LIMIT 1&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT INTO syst.dc_natureza_juridica&#x28;&#xd;&#xa;            cd_natureza_juridica, tx_nome_natureza_juridica&#x29;&#xd;&#xa;SELECT 3263, &#x27;&#xd3;rg&#xe3;o de Dire&#xe7;&#xe3;o Regional de Partido Pol&#xed;tico&#x27;&#xd;&#xa;FROM syst.dc_natureza_juridica&#xd;&#xa;WHERE NOT EXISTS &#x28;SELECT &#x2a; FROM syst.dc_natureza_juridica WHERE cd_natureza_juridica &#x3d; 3263&#x29;&#xd;&#xa;LIMIT 1&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT INTO syst.dc_natureza_juridica&#x28;&#xd;&#xa;            cd_natureza_juridica, tx_nome_natureza_juridica&#x29;&#xd;&#xa;SELECT 3298, &#x27;Frente Plebiscit&#xe1;ria ou Referend&#xe1;ria&#x27;&#xd;&#xa;FROM syst.dc_natureza_juridica&#xd;&#xa;WHERE NOT EXISTS &#x28;SELECT &#x2a; FROM syst.dc_natureza_juridica WHERE cd_natureza_juridica &#x3d; 3298&#x29;&#xd;&#xa;LIMIT 1&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT INTO syst.dc_natureza_juridica&#x28;&#xd;&#xa;            cd_natureza_juridica, tx_nome_natureza_juridica&#x29;&#xd;&#xa;SELECT 3255, &#x27;&#xd3;rg&#xe3;o de Dire&#xe7;&#xe3;o Nacional de Partido Pol&#xed;tico&#x27;&#xd;&#xa;FROM syst.dc_natureza_juridica&#xd;&#xa;WHERE NOT EXISTS &#x28;SELECT &#x2a; FROM syst.dc_natureza_juridica WHERE cd_natureza_juridica &#x3d; 3255&#x29;&#xd;&#xa;LIMIT 1&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT INTO syst.dc_natureza_juridica&#x28;&#xd;&#xa;            cd_natureza_juridica, tx_nome_natureza_juridica&#x29;&#xd;&#xa;SELECT 3280, &#x27;Comit&#xea; Financeiro de Partidos Pol&#xed;ticos&#x27;&#xd;&#xa;FROM syst.dc_natureza_juridica&#xd;&#xa;WHERE NOT EXISTS &#x28;SELECT &#x2a; FROM syst.dc_na