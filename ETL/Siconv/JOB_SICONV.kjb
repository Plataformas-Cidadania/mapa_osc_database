      <name>Criar variáveis</name>
      <description />
      <type>EVAL</type>
      <script>date = new java.util.Date();
date.setDate(date.getDate());

var ano = new java.text.SimpleDateFormat("yyyy").format(date);
var mes = new java.text.SimpleDateFormat("MM").format(date);
var dia = new java.text.SimpleDateFormat("dd").format(date);

var diretorio_base = parent_job.getVariable("Internal.Job.Filename.Directory", "").split(":///")[1];
var diretorio_download = diretorio_base.concat("/Download/") + ano + "/" + mes + "/" + dia;

parent_job.setVariable("DIRETORIO_DOWNLOAD", diretorio_download);

parent_job.setVariable("PROPOSTAS_URL", "http://portal.convenios.gov.br/images/docs/CGSIS/csv/siconv_proposta.csv.zip");
parent_job.setVariable("PROPOSTAS_ZIP", diretorio_download + "/siconv_proposta.csv.zip");
parent_job.setVariable("PROPOSTAS_CSV", diretorio_download + "/siconv_proposta.csv");

parent_job.setVariable("CONVENIOS_URL", "http://portal.convenios.gov.br/images/docs/CGSIS/csv/siconv_convenio.csv.zip");
parent_job.setVariable("CONVENIOS_ZIP", diretorio_download + "/siconv_convenio.csv.zip");
parent_job.setVariable("CONVENIOS_CSV", diretorio_download + "/siconv_convenio.csv");
parent_job.setVariable("SICONV_CSV", diretorio_download + "/siconv.csv");

parent_job.setVariable("FONTE_CARGA", "SICONV/MPOG " + dia + "-" + mes + "-" + ano);

true;</script>
