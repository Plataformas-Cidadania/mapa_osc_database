    <sql>UPDATE osc.tb_dados_gerais SET tx_razao_social_osc = '?', ft_razao_social_osc = '?'
WHERE id_osc = (SELECT id_osc FROM osc.tb_osc WHERE cd_identificador_osc = ?::NUMERIC);


UPDATE osc.tb_projeto SET cd_status_projeto = '?', ft_status_projeto = '?', tx_descricao_projeto = RTRIM('?'), ft_descricao_projeto = '?', dt_data_inicio_projeto = to_date('?','DD/MM/YYYY'), ft_data_inicio_projeto = '?', dt_data_fim_projeto = to_date('?','DD/MM/YYYY'),
							ft_data_fim_projeto = '?', nr_valor_total_projeto = replace(replace('?','.',''),',','.')::DOUBLE PRECISION, ft_valor_total_projeto = '?', bo_oficial = true, tx_nome_projeto = '?',
							ft_nome_projeto = '?', id_osc = a.id_osc, ft_identificador_projeto_externo = '?'
FROM osc.tb_osc a 
WHERE cd_identificador_osc = ?::NUMERIC
AND (tx_identificador_projeto_externo = '?' AND ft_identificador_projeto_externo = '?');


INSERT INTO osc.tb_projeto (cd_status_projeto, ft_status_projeto, tx_descricao_projeto, ft_descricao_projeto, dt_data_inicio_projeto, ft_data_inicio_projeto, dt_data_fim_projeto,
							ft_data_fim_projeto, nr_valor_total_projeto, ft_valor_total_projeto, tx_identificador_projeto_externo, bo_oficial, tx_nome_projeto, ft_nome_projeto, id_osc, ft_identificador_projeto_externo)
SELECT '?', '?', RTRIM('?'), '?', to_date('?','DD/MM/YYYY'), '?', to_date('?','DD/MM/YYYY'), '?', replace(replace('?','.',''),',','.')::DOUBLE PRECISION, '?', '?', true, '?', '?', a.id_osc, '?'
FROM osc.tb_osc a
WHERE cd_identificador_osc = ?::NUMERIC
AND NOT EXISTS (SELECT * FROM osc.tb_projeto WHERE osc.tb_projeto.tx_identificador_projeto_externo = '?' AND ft_identificador_projeto_externo = '?');

