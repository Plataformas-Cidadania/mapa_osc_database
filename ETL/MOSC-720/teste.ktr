        <jsScript_script>function formatarDado(dado) {
	var resultado = String(dado);
	
	resultado = resultado.replace(/'/g, '\'\'');
	resultado =	JSON.stringify(resultado);
	
	return resultado;
}

if(NO_RAZAO_SOCIAL) NO_RAZAO_SOCIAL = formatarDado(NO_RAZAO_SOCIAL);
if(NO_FANTASIA) NO_FANTASIA = formatarDado(NO_FANTASIA);
if(CO_NATUREZA_JUR) CO_NATUREZA_JUR = formatarDado(CO_NATUREZA_JUR);
if(DS_TIPO_UNIDADE) DS_TIPO_UNIDADE = formatarDado(DS_TIPO_UNIDADE);
if(DS_GESTAO) DS_GESTAO = formatarDado(DS_GESTAO);

if(NO_LOGRADOURO) NO_LOGRADOURO = formatarDado(NO_LOGRADOURO);
if(NO_COMPLEMENTO) NO_COMPLEMENTO = formatarDado(NO_COMPLEMENTO);
if(NU_ENDERECO) NU_ENDERECO = formatarDado(NU_ENDERECO);
if(NO_BAIRRO) NO_BAIRRO = formatarDado(NO_BAIRRO);
if(CO_CEP) CO_CEP = formatarDado(CO_CEP);

var area_atuacao_json = '{"cd_area_atuacao": ' + area_atuacao + '}';
var dados_gerais_json = '{"tx_razao_social_osc": ' + NO_RAZAO_SOCIAL + ', "tx_nome_fantasia_osc": ' + NO_FANTASIA + ', "cd_natureza_juridica_osc": ' + CO_NATUREZA_JUR + ', "tx_tipo_unidade_saude": ' + DS_TIPO_UNIDADE + ', "tx_tipo_gestao": ' + DS_GESTAO + '}';
var localizacao_json = '{"cd_municipio": ' + edmu_cd_municipio + ', "tx_endereco": ' + NO_LOGRADOURO + ', "nr_localizacao": ' + NU_ENDERECO + ', "tx_bairro": ' + NO_BAIRRO + ', "nr_cep": ' + CO_CEP + '}';</jsScript_script>
