UPDATE osc.tb_dados_gerais SET ft_finalidades_estatutarias = null WHERE ft_finalidades_estatutarias is not null and ft_finalidades_estatutarias &lt;&gt; 'Representante de OSC' and tx_finalidades_estatutarias is null;
