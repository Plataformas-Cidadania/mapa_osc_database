    <entry>
      <name>SQL</name>
      <description />
      <type>SQL</type>
      <sql>ALTER TABLE osc.tb_dados_gerais DISABLE TRIGGER ALL; 

UPDATE osc.tb_dados_gerais SET tx_nome_fantasia_osc = tb.nome
FROM
(
--select id_osc,convert_to(unaccent(regexp_replace(tx_nome_fantasia_osc, '[^a-zA-Zà-úÀ-Ú0-9+?/:().,"''&amp;@ -]','','g')),'utf8') as nome, tx_nome_fantasia_osc
--from osc.tb_dados_gerais where tx_nome_fantasia_osc ~ '[^a-zA-Zà-úÀ-Ú0-9+?/:().,"''&amp;@ -]'
--union all
--select id_osc,convert_to(unaccent(regexp_replace(tx_nome_fantasia_osc, '[^a-zA-Z0-9+?/:(),"''&amp;@ -]','','g')),'utf8') as nome, tx_nome_fantasia_osc
--from osc.tb_dados_gerais where tx_nome_fantasia_osc = '.'
--union all
--select id_osc,convert_to(unaccent(regexp_replace(tx_nome_fantasia_osc, '[^a-zA-Z0-9+?/:(),"''&amp;@ -]',' ','g')),'utf8') as nome, tx_nome_fantasia_osc
--from osc.tb_dados_gerais where tx_nome_fantasia_osc like '%...%'
--union all
--select id_osc,convert_to(unaccent(regexp_replace(tx_nome_fantasia_osc, '[^a-zA-Z0-9+?/:(),"''&amp;@ ]','','g')),'utf8') as nome, tx_nome_fantasia_osc
--from osc.tb_dados_gerais where tx_nome_fantasia_osc = '-'
--union all
--select id_osc,convert_to(unaccent(regexp_replace(tx_nome_fantasia_osc, '[^a-zA-Z0-9+?/:()"''&amp;@ ]','','g')),'utf8') as nome, tx_nome_fantasia_osc
--from osc.tb_dados_gerais where tx_nome_fantasia_osc = ','
--union all
--select id_osc,convert_to(unaccent(regexp_replace(tx_nome_fantasia_osc, '[^a-zA-Z+?/:()"''&amp;@ ]','','g')),'utf8') as nome, tx_nome_fantasia_osc
--from osc.tb_dados_gerais where tx_nome_fantasia_osc like '000%'
--union all
--select id_osc,null nome, tx_nome_fantasia_osc
--from osc.tb_dados_gerais where trim(tx_nome_fantasia_osc) in('-0-','--','---','----','-------','-------------','--------------------------------------')
--union all
select id_osc,regexp_replace(tx_nome_fantasia_osc, '[@|-|;|_|*|\|()|!|?|#|$|%|&amp;|''|"]',' ','g') as nome, tx_nome_fantasia_osc
from osc.tb_dados_gerais where tx_nome_fantasia_osc ~ '[@|-|;|_|*|\|()|!|?|#|$|%|&amp;|''|"]'
union all
select id_osc,regexp_replace(tx_nome_fantasia_osc, '[-]',' ','g') as nome, tx_nome_fantasia_osc
from osc.tb_dados_gerais where tx_nome_fantasia_osc = '- - -'
union all
select id_osc,regexp_replace(tx_nome_fantasia_osc, '[-]',' ','g') as nome, tx_nome_fantasia_osc
from osc.tb_dados_gerais where tx_nome_fantasia_osc = '------------------------------'
) as tb
WHERE osc.tb_dados_gerais.id_osc = tb.id_osc;

UPDATE osc.tb_dados_gerais SET tx_nome_fantasia_osc = null
where length(tx_nome_fantasia_osc) &lt;= 2;

UPDATE osc.tb_dados_gerais SET tx_razao_social_osc = regexp_replace(tx_razao_social_osc, '[@|-|;|_|*|\|()|.|!|?|#|$|%|&amp;|''|"|:]',' ','g')
WHERE tx_razao_social_osc ~ '[@|-|;|_|*|\|()|.|!|?|#|$|%|&amp;|''|"|:]';


UPDATE osc.tb_dados_gerais SET ft_nome_fantasia_osc = null WHERE ft_nome_fantasia_osc is not null and ft_nome_fantasia_osc &lt;&gt; 'Representante de OSC' and tx_nome_fantasia_osc is null;
UPDATE osc.tb_dados_gerais SET ft_logo = null WHERE ft_logo is not null and ft_logo &lt;&gt; 'Representante de OSC' and im_logo is null;
UPDATE osc.tb_dados_gerais SET ft_missao_osc = null WHERE ft_missao_osc is not null and ft_missao_osc &lt;&gt; 'Representante de OSC' and tx_missao_osc is null;
UPDATE osc.tb_dados_gerais SET ft_visao_osc = null WHERE ft_visao_osc is not null and ft_visao_osc &lt;&gt; 'Representante de OSC' and tx_visao_osc is null;
UPDATE osc.tb_dados_gerais SET ft_sigla_osc = null WHERE ft_sigla_osc is not null and ft_sigla_osc &lt;&gt; 'Representante de OSC' and tx_sigla_osc is null;
UPDATE osc.tb_dados_gerais SET ft_resumo_osc = null WHERE ft_resumo_osc is not null and ft_resumo_osc &lt;&gt; 'Representante de OSC' and tx_resumo_osc is null;
UPDATE osc.tb_dados_gerais SET ft_situacao_imovel_osc = null WHERE ft_situacao_imovel_osc is not null and ft_situacao_imovel_osc &lt;&gt; 'Representante de OSC' and cd_situacao_imovel_osc is null;
UPDATE osc.tb_dados_gerais SET ft_link_estatuto_osc = null WHERE ft_link_estatuto_osc is not null and ft_link_estatuto_osc &lt;&gt; 'Representante de OSC' and tx_link_estatuto_osc is null;
