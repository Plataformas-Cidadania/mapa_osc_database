      <name>SQL</name>
      <description />
      <type>SQL</type>
      <sql>ALTER TABLE osc.tb_dados_gerais DISABLE TRIGGER ALL; 

UPDATE osc.tb_dados_gerais SET tx_nome_fantasia_osc = tb.nome
FROM
(select id_osc,convert_to(unaccent(regexp_replace(tx_nome_fantasia_osc, '[^a-zA-Zà-úÀ-Ú0-9+?/:().,"''&amp;@ -]','','g')),'utf8') as nome, tx_nome_fantasia_osc
from osc.tb_dados_gerais where tx_nome_fantasia_osc ~ '[^a-zA-Zà-úÀ-Ú0-9+?/:().,"''&amp;@ -]'
union all
select id_osc,convert_to(unaccent(regexp_replace(tx_nome_fantasia_osc, '[^a-zA-Z0-9+?/:(),"''&amp;@ -]','','g')),'utf8') as nome, tx_nome_fantasia_osc
from osc.tb_dados_gerais where tx_nome_fantasia_osc = '.'
union all
select id_osc,convert_to(unaccent(regexp_replace(tx_nome_fantasia_osc, '[^a-zA-Z0-9+?/:(),"''&amp;@ -]',' ','g')),'utf8') as nome, tx_nome_fantasia_osc
from osc.tb_dados_gerais where tx_nome_fantasia_osc like '%...%'
union all
select id_osc,convert_to(unaccent(regexp_replace(tx_nome_fantasia_osc, '[^a-zA-Z0-9+?/:(),"''&amp;@ ]','','g')),'utf8') as nome, tx_nome_fantasia_osc
from osc.tb_dados_gerais where tx_nome_fantasia_osc = '-'
union all
select id_osc,convert_to(unaccent(regexp_replace(tx_nome_fantasia_osc, '[^a-zA-Z0-9+?/:()"''&amp;@ ]','','g')),'utf8') as nome, tx_nome_fantasia_osc
from osc.tb_dados_gerais where tx_nome_fantasia_osc = ','
union all
select id_osc,convert_to(unaccent(regexp_replace(tx_nome_fantasia_osc, '[^a-zA-Z+?/:()"''&amp;@ ]','','g')),'utf8') as nome, tx_nome_fantasia_osc
from osc.tb_dados_gerais where tx_nome_fantasia_osc like '000%'
union all
select id_osc,null nome, tx_nome_fantasia_osc
from osc.tb_dados_gerais where trim(tx_nome_fantasia_osc) in('-0-','--','---','----','-------','-------------','--------------------------------------')
) as tb
WHERE osc.tb_dados_gerais.id_osc = tb.id_osc;

UPDATE osc.tb_dados_gerais SET tx_nome_fantasia_osc = null
where length(tx_nome_fantasia_osc) &lt;= 2;

ALTER TABLE osc.tb_dados_gerais ENABLE TRIGGER ALL; 

update osc.tb_localizacao SET geo_localizacao = edmu_centroid
from spat.ed_municipio
inner join spat.ed_uf on spat.ed_municipio.eduf_cd_uf = spat.ed_uf.eduf_cd_uf
inner join osc.tb_osc on osc.tb_osc.id_osc = id_osc
where ST_Contains(eduf_bounding_box,geo_localizacao) = false
and cd_municipio = edmu_cd_municipio;

update osc.tb_localizacao SET geo_localizacao = edmu_centroid
from spat.ed_municipio
inner join spat.ed_uf on spat.ed_municipio.eduf_cd_uf = spat.ed_uf.eduf_cd_uf
inner join osc.tb_osc on osc.tb_osc.id_osc =id_osc
where ST_Contains(edmu_bounding_box,geo_localizacao) = false
and cd_municipio = edmu_cd_municipio;

select RefreshAllMaterializedViews('spat');
select RefreshAllMaterializedViews('osc');
select RefreshAllMaterializedViews('portal');</sql>
