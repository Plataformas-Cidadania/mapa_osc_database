    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>function formatarDado(dado) {
	var resultado = String(dado);
	
	resultado = resultado.replace(/'/g, '\'\'');
	resultado =	JSON.stringify(resultado);
	
	return resultado;
}

if(VALIDADE INICIO) VALIDADE INICIO = formatarDado(VALIDADE INICIO);
if(VALIDADE FIM) VALIDADE FIM = formatarDado(VALIDADE FIM);
if(MUNICÍPIO) MUNICÍPIO = formatarDado(MUNICÍPIO);
if(ENTIDADE) ENTIDADE = formatarDado(ENTIDADE);

var area_atuacao_json = '{"cd_area_atuacao": ' + area_atuacao + ', "cd_subarea_atuacao": ' + subarea_atuacao + '}';
var certificado_json = '{"cd_certificado": ' + certificado + ', "dt_data_inicio": ' + VALIDADE INICIO + ', "dt_data_inicio": ' + VALIDADE FIM + '}';
var localizacao_json = '{"cd_municipio": ' + MUNICÍPIO + '}';
var dados_gerais_json = '{"tx_razao_social_osc": ' + ENTIDADE + '}';
</jsScript_script>
      </jsScript>
