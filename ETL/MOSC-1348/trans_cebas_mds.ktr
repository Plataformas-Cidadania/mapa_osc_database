    <sql>INSERT INTO osc.tb_area_atuacao(id_osc, cd_area_atuacao, cd_subarea_atuacao, ft_area_atuacao, bo_oficial)
SELECT a.id_osc, 5, 17, 'CEBAS/MDSA', false
FROM osc.tb_osc a
WHERE a.cd_identificador_osc = ?::NUMERIC
AND NOT EXISTS (SELECT * FROM osc.tb_area_atuacao b WHERE a.id_osc = b.id_osc AND b.cd_area_atuacao = 5 AND b.cd_subarea_atuacao = 17);

UPDATE osc.tb_certificado SET dt_inicio_certificado = to_date('?','DD/MM/YYYY'), ft_inicio_certificado = 'CEBAS/MDSA', dt_fim_certificado = to_date('?','DD/MM/YYYY'), ft_fim_certificado = 'CEBAS/MDSA'
FROM osc.tb_osc
WHERE osc.tb_certificado.id_osc = osc.tb_osc.id_osc
